package com.utils;

import org.openqa.selenium.WebDriver;

public class AppWebDriver {

	private WebDriver driver;
	private String driverProperty;
	private String driverPath;
	
	public AppWebDriver(WebDriver driver, String property, String path){
		this.setDriver(driver);
		this.setDriverProperty(property);
		this.setDriverPath(path);
	}

	public WebDriver getDriver() {
		return driver;
	}

	private void setDriver(WebDriver driver) {
		this.driver = driver;
	}

	public String getDriverProperty() {
		return driverProperty;
	}

	private void setDriverProperty(String driverProperty) {
		this.driverProperty = driverProperty;
	}

	private String getDriverPath() {
		return driverPath;
	}

	private void setDriverPath(String driverPath) {
		this.driverPath = driverPath;
	}
	
}
